<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/bottom_navigation"
        app:itemBackground="@color/colorPrimary"
        app:itemTextColor="@drawable/selector"
        app:itemIconTint="@drawable/selector"
        app:menu="@menu/menu_navigation"/>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/bottom_navigation"
        android:layout_marginTop="30px"
        android:layout_marginRight="50px"
        android:layout_marginLeft="50px">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/text0"
            android:textSize="16dp"
            android:textStyle="bold"
            android:textColor="#000000"
            android:layout_centerHorizontal="true"
            android:text="ONLINE JIPMER HOSPITAL APPOINTMENTS"></TextView>

        <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/text"
            android:layout_below="@id/text0"
            android:layout_marginTop="10dp"
        android:textSize="16dp"
        android:textColor="#000000"
        android:layout_centerHorizontal="true"
        android:text="ஜிப்மர் மருத்துவத் துறைகளில் முன் பதிவு செய்வதற்கான கைப்பேசி  எண்கள்"></TextView>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/text"
        android:layout_marginTop="10dp"
        android:id="@+id/text1"
        android:textSize="15dp"
        android:layout_centerHorizontal="true"
        android:textColor="#000000"
        android:text="An outcome of Developer Student's Club (Pondicherry Engineering Collge)"></TextView>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/text1"
        android:layout_marginTop="10dp"
        android:id="@+id/text2"
        android:textColor="#000000"
        android:textSize="15dp"
        android:layout_centerHorizontal="true"
        android:textStyle="bold"
        android:text="MENTOR"></TextView>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/text2"
        android:layout_marginTop="10dp"
        android:id="@+id/text3"
        android:textSize="15dp"
        android:layout_centerHorizontal="true"
        android:textColor="#000000"
        android:text="Dr.Ka.Selvaradjou (Professor CSE)"></TextView>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/text3"
        android:layout_marginTop="10dp"
        android:id="@+id/text4"
        android:textSize="15dp"
        android:textColor="#000000"
        android:layout_centerHorizontal="true"
        android:textStyle="bold"
        android:text="DEVELOPERS"></TextView>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/text4"
        android:layout_marginTop="10dp"
        android:id="@+id/text5"
        android:textSize="15dp"
        android:layout_centerHorizontal="true"
        android:textColor="#000000"
        android:text="S.Swetha"></TextView>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/text5"
        android:layout_marginTop="10dp"
        android:id="@+id/text6"
        android:textSize="15dp"
        android:textColor="#000000"
        android:layout_centerHorizontal="true"
        android:text="Krishna Kumar Davuluri"></TextView>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/text6"
            android:layout_marginTop="10dp"
            android:id="@+id/text7"
            android:textSize="15dp"
            android:textColor="#000000"
            android:layout_centerHorizontal="true"
            android:textStyle="bold"
            android:text="GUIDED BY"></TextView>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/text7"
            android:layout_marginTop="10dp"
            android:id="@+id/text8"
            android:textSize="15dp"
            android:textColor="#000000"
            android:layout_centerHorizontal="true"
            android:text="Durga Prasad Palla"></TextView>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/text8"
        android:layout_marginTop="10dp"
        android:id="@+id/text9"
        android:textColor="#000000"
        android:textSize="15dp"
        android:layout_centerHorizontal="true"
        android:textStyle="bold"
        android:text="RELEASE DATE"></TextView>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/text9"
        android:layout_marginTop="10dp"
        android:id="@+id/text10"
        android:textSize="15dp"
        android:textColor="#000000"
        android:layout_centerHorizontal="true"
        android:text="Version 1.0 on"></TextView>
    </RelativeLayout>
</RelativeLayout>